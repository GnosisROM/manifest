<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="github"
           fetch="https://github.com/" />

  <remote  name="gitlab"
           fetch="https://gitlab.com/" />

  <remote  name="gnosis"
           fetch="https://github.com/GnosisROM"
           revision="refs/heads/11" />


  <!-- Build -->
  <remove-project name="platform/bionic" />
  <project path="bionic" name="bionic" remote="gnosis" />
  <remove-project name="platform/build" />
  <project path="build/make" name="build" groups="pdk" remote="gnosis" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>

  <!-- Device -->
  <remove-project name="device/common" />
  <project path="device/common" name="device_common" remote="gnosis" />
  <remove-project name="device/google/bramble" />
  <project path="device/google/bramble" name="device_google_bramble" remote="gnosis" />
  <remove-project name="device/google/redbull" />
  <project path="device/google/redbull" name="device_google_redbull" remote="gnosis" />
  <remove-project name="device/google/redbull-sepolicy" />
  <project path="device/google/redbull-sepolicy" name="device_google_redbull-sepolicy" remote="gnosis" />
  <remove-project name="device/google/redfin" />
  <project path="device/google/redfin" name="device_google_redfin" remote="gnosis" />

  <!-- External -->

  <!-- Frameworks -->
  <remove-project name="platform/frameworks/base" />
  <project path="frameworks/base" name="frameworks_base" remote="gnosis" />
  <remove-project name="platform/frameworks/native" />
  <project path="frameworks/native" name="frameworks_native" remote="gnosis" />



  <!-- Hardware -->
  <project path="hardware/qcom/msm8998" name="hardware_qcom_msm8998" remote="gnosis" groups="gnosis" />


  <!-- Kernel -->
  <!--project path="kernel/google/bramble" name="kernel_google_bramble" remote="gnosis" /-->

  <!-- Manifest -->
  <project path="manifest" name="manifest" remote="gnosis" />

  <!-- Native -->

  <!-- Packages -->
  <project path="packages/apps/ElmyraService" name="packages_apps_ElmyraService" remote="gnosis" />
  <project path="packages/apps/Jelly" name="packages_apps_Jelly" remote="gnosis" />
  <remove-project name="platform/packages/apps/Settings" />
  <project path="packages/apps/Settings" name="packages_apps_Settings" remote="gnosis" />
  <remove-project name="platform/packages/apps/Terminal" />
  <project path="packages/apps/Terminal" name="packages_apps_Terminal" remote="gnosis" />


  <!-- Prebuilts -->
  <project path="prebuilts/tools-lineage"  name="prebuilts_tools-lineage" remote="gnosis" groups="gnosis" />

  <!-- System -->
  <remove-project name="platform/system/core" />
  <project path="system/core" name="system_core" remote="gnosis" groups="gnosis" />
  <remove-project name="platform/system/hardware/interfaces" />
  <project path="system/hardware/interfaces" name="system_hardware_interfaces" remote="gnosis" groups="gnosis" />
  <remove-project name="platform/system/sepolicy" />
  <project path="system/sepolicy" name="system_sepolicy" remote="gnosis" groups="gnosis" />

  <!-- Vendor -->
  <project path="vendor/gapps" name="vendor_gapps" remote="gnosis" />
  <project path="vendor/google" name="vendor_google" remote="gnosis" />
  <project path="vendor/gnosis" name="vendor_gnosis" remote="gnosis" />
  <project path="vendor/google_devices/bramble" name="vendor_google_bramble" remote="gnosis" />

</manifest>
